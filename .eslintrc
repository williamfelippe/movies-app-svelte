    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "plugins": [
        "svelte3"
    ],
    "rules": {
      "indent": ["error", 4],
      "semi": ["error", "never"]
    },
    "settings": {}
}
